<h1 align="center"> List all Category </h1>
<div align ="center">
<%= will_paginate %>
<% @categories.each do |category| %>
    <ul class="listing">
        <div class="row">
            <div class="well col-md-4 col-md-offset-4">
                <li class="article-title">
                   <div align="center"><%= link_to "#{category.name}", category_path(category) %></%></div> 
                </li>
            </div>
        </div>
    </ul>
<%end%>
<%= will_paginate %>
</div>