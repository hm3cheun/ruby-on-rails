<% if obj.errors.any? %>
<div class="row col-xs-8 col-xs-offset-2 panel panel-danger panel-heading">
   <h2 class="panel-title"> <%= pluralize(obj.errors.count, "error") %> Prohibits from being saved</h2>
<%end%>

<div class="panel-body">
     <ul>
        <% obj.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <%end%>
     </ul>
     </div>
</div>